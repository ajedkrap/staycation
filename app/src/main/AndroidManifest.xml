<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="id.ajedkrap.staycation">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_staycation"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_staycation_round"
        android:supportsRtl="true"
        >
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:theme="@style/Theme.Staycation"
            >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ListCityActivity"
            android:exported="false"
            android:theme="@style/Theme.Staycation"
            >

        </activity>
        <activity
            android:name=".CityDetailActivity"
            android:exported="false"
            android:theme="@style/Theme.StaycationTranslucent"
            />


    </application>

</manifest>